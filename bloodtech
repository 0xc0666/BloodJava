    UI.SetEnabled("Script items", "Peek Helper for AWP/Scout", menu_rage)
    UI.SetEnabled("Script items", "Disable fakelags on r8", menu_rage)
    UI.SetEnabled("Script items", "Adaptive autoscope", menu_rage)
    UI.SetEnabled("Script items", "pingspike", menu_rage)
    UI.SetEnabled("Script items", "Dormant Aimbot", menu_rage)
    UI.SetEnabled("Script items", "Force kill damage", menu_rage)
    UI.SetEnabled("Script items", "Weapon Override", menu_rage)
    UI.SetEnabled("Script items", "Weapon Mindmg", menu_rage)
    UI.SetEnabled("Script items", "Jump scout/r8", menu_rage)
    UI.SetEnabled("Script items", "Jump scout hitchance", menu_rage && UI.GetValue("Script items", "Jump scout/r8"))
    UI.SetEnabled("Script items", "Jump revolver hitchance", menu_rage && UI.GetValue("Script items", "Jump scout/r8"))

    UI.SetEnabled("Script items", "legfucker", menu_aa)
